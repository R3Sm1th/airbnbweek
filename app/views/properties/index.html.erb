<div class="container">
  <h4>Propety Listings</h4>
  <div>
    <div class="card-deck">
      <% @properties.each do |property| %>
      <div class="card">
        <%= cl_image_tag property.photos[0].key, class:"card-img-top", alt:"Card image cap" %>
        <div class="card-body">
          <h5 class="card-title"> <%= property.name %></h5>
          <p class="card-text"><%= property.address %>, <%= property.postcode %></p>
          <%= link_to "Show", property_path(property.id)%>
        </div>
        <div class="card-footer">
          <small class="text-muted">Rating <%= property.rating %></small>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>
